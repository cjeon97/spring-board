<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="add" class="exam3.AddCalculator"/>
	<bean id="min" class="exam3.MinCalculator"/>

	<bean id="cbAdd" class="exam3.CalculatorBody">
		<property name="calculator" ref="add"/>
	</bean>
	
	<bean id="cbMin" class="exam3.CalculatorBody">
		<property name="calculator" ref="min"/>
	</bean>
	
</beans>
