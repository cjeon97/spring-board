package usemodule;

import mycalc.AddCalculator;

public class Exam {

	public static void main(String[] args) {
		
		AddCalculator ac = new AddCalculator();
		System.out.println(ac.calc(1,2));
		System.out.println(ac.calc(2,3));
		System.out.println(ac.calc(12,45));
		System.out.println(ac.calc(432,3));
		System.out.println(ac.getCount());

		
	}
	
}
