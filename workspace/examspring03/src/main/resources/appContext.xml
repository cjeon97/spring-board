<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:context="http://www.springframework.org/schema/context"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans.xsd
  http://www.springframework.org/schema/context 
  https://www.springframework.org/schema/context/spring-context.xsd">
  
  	<context:annotation-config/>

	<bean id="memberDao" class="examspring03.MemberDao"/>
	
	<bean id="memberRegisterService" class="examspring03.MemberRegisterService"/>

	
	<bean id="memberPrintService1" class="examspring03.MemberPrintService">
		<property name="type" value="stdout"/>
		<qualifier value="stdout"/>
	</bean>
	<bean id="memberPrintService2" class="examspring03.MemberPrintService">
		<property name="type" value="stderr"/>
		<qualifier value="stderr"/>
	</bean>

	
	<bean id="memberSearchService" class="examspring03.MemberSearchService"/>

	
</beans>
